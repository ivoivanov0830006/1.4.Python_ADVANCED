men_count = int(input())
women_count = int(input())
tables_count = int(input())
total_tables = 0
is_full = False

for man in range(1, men_count + 1):
    for woman in range(1, women_count + 1):
        total_tables += 1
        print(f"({man} <-> {woman})", end=" ")
        if total_tables == tables_count:
            is_full = True
            break
    if is_full:
        break
